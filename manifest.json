{
    "name": "Watcher",
    "version": "1.0",
    "description": "This extension aims to analyze websites using Decision Tree concepts.",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icon128.png"
    },
    "permissions": [
        "tabs",
        "http://*/*"
    ],
    "web_accessible_resources":[
        "styles/*",
        "script.js"
      ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2
}